{
  "name": "cloud-iam-keycloak-theme",
  "version": "1.0.0",
  "keywords": [
    "keycloak",
    "theme",
    "keycloak-theme"
  ],
  "description": "Cloud-IAM Theme for Keycloak",
  "scripts": {
    "build": "PARCEL_WORKER_BACKEND=process parcel build --dist-dir=./src/main/resources/theme/wundergraph/login/resources/dist/ src/main/resources/theme/wundergraph/login/resources/scss/*.scss",
    "watch": "nodemon -w src/ --ignore 'dist/*' -e \"properties,js,ftl,scss\" --exec \"bash ./build-and-restart-keycloak.sh\""
  },
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com/)",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-image": "^2.3.2",
    "@parcel/transformer-sass": "^2.3.2",
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^2.0.15",
    "parcel": "^2.3.2",
    "postcss": "^8.4.6",
    "postcss-fontpath": "^1.0.0",
    "tailwindcss": "^3.0.23"
  },
  "dependencies": {}
}
